{
	"name": "top-of-the-flocks",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"db:schema:pull": "supabase migration fetch --yes && supabase db pull || true",
		"db:schema:build": "{ echo \"BEGIN;\"; echo \"DROP SCHEMA IF EXISTS public CASCADE;\"; cat ./supabase/schema/*.sql; echo \"COMMIT;\"; }> ./supabase/schema_build.sql",
		"db:schema:apply": "npm run db:schema:build && psql \"postgresql://postgres:postgres@localhost:54322/postgres\" -f ./supabase/schema_build.sql && echo 'local schema applied to local db'",
		"db:migration:prepare": "npm run db:schema:pull && npm run db:schema:apply && supabase db diff --local -f $(git rev-parse --abbrev-ref HEAD) && npm run db:types",
		"db:diff": "npm run db:schema:pull && supabase db diff --local",
		"db:migration:push": "supabase migration up --linked --debug --include-all",
		"db:sync:local": "npm run db:schema:pull && supabase db reset && npm run db:seed:local",
		"db:seed:local": "npm run db:import:local ./data/end25.csv",
		"db:local:start": "supabase start",
		"db:local:console": "open http://localhost:54323/project/default",
		"db:types": "supabase gen types typescript --local --schema public > ./types/supabase.types.ts",
		"db:import:prod": "./scripts/load-prod-env.sh tsx scripts/import-csv.ts",
		"db:import:local": "dotenvx run -f .env.dev -- tsx scripts/import-csv.ts",
		"start": "next start",
		"build": "next build --turbopack",
		"next:dev": "dotenvx run -f .env.dev -- next dev --turbopack",
		"next:prod": "./scripts/load-prod-env.sh next dev --turbopack",
		"next:build": "dotenvx run -f .env.dev -- next build --turbopack --debug",
		"lint": "npx prettier -w app supabase/schema scripts lib && npx eslint",
		"type:check": "tsc --noEmit",
		"type:check:open": "tsx scripts/open-type-errors.ts",
		"qa:bloat": "find app -type f -exec wc -l {} + | awk '$1 > 150 {print $1, $2}'",
		"test": "dotenvx run -f .env.dev -- vitest",
		"test:nowatch": "dotenvx run -f .env.dev -- vitest run",
		"qa": "npm run lint && npm run type:check && npm run test:nowatch",
		"prepare": "husky"
	},
	"dependencies": {
		"@iconify-json/fluent-emoji-flat": "^1.2.5",
		"@supabase/supabase-js": "^2.75.1",
		"@tailwindcss/postcss": "^4.1.18",
		"chart.js": "^4.5.1",
		"date-fns": "^4.1.0",
		"flyonui": "^2.4.1",
		"next": "^16.1.4",
		"p-ratelimit": "^1.0.1",
		"postcss": "^8.5.6",
		"prettier": "^3.7.4",
		"react": "19.1.0",
		"react-chartkick": "^0.5.4",
		"react-dom": "19.1.0",
		"react-intersection-observer": "^10.0.2",
		"tailwindcss": "^4.1.18"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "^1.52.0",
		"@eslint/eslintrc": "^3",
		"@iconify-json/tabler": "^1.2.26",
		"@iconify/tailwind4": "^1.2.1",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/react": "^16.3.2",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@vitejs/plugin-react": "^5.1.3",
		"csv-parser": "^3.2.0",
		"eslint": "^9",
		"eslint-config-next": "15.5.6",
		"husky": "^9.1.7",
		"jsdom": "^28.0.0",
		"jsdom-testing-mocks": "^1.16.0",
		"prettier-plugin-sql": "^0.19.2",
		"supabase": "^2.72.8",
		"tsx": "^4.20.6",
		"typescript": "^5",
		"vite-tsconfig-paths": "^6.0.5",
		"vitest": "^4.0.18",
		"yaml": "^2.4.0"
	}
}
